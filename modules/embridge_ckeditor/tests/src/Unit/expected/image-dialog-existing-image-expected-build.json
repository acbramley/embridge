{
  "#tree": true,
  "#attached": {
    "library": [
      "editor\/drupal.editor.dialog"
    ]
  },
  "#prefix": "\u003Cdiv id=\u0022embridge-ckeditor-image-dialog-form\u0022\u003E",
  "#suffix": "\u003C\/div\u003E",
  "asset": {
    "preview": {
      "#theme": "image",
      "#uri": "www.example.com/test_application/path/to/preview/thumb.png",
      "#weight": "-100"
    },
    "#title": "Image",
    "#type": "embridge_asset",
    "#catalog_id": "test_catalog",
    "#upload_location": "public:\/\/test-directory",
    "#default_value": [
      78931279123987312
    ],
    "#upload_validators": {
      "validateFileExtensions": [
        "gif png jpg jpeg"
      ],
      "validateFileSize": [
        2097152
      ]
    },
    "#pre_render": [
      [
        "Drupal\\embridge_ckeditor\\Form\\EmbridgeCkeditorImageDialog",
        "preRenderAssetElement"
      ]
    ],
    "#allow_search": false,
    "#required": true,
    "search_link": {
      "#type": "link",
      "#url": {},
      "#title": "Search asset library"
    }
  },
  "attributes_wrapper": {
    "#type": "container",
    "#tree": true,
    "#attributes": {
      "class": ["image-attributes"]
    },
    "attributes": {
      "src": {
        "#type": "value"
      },
      "alt": {
        "#title": "Alternative text",
        "#description": "The alt text describes the image for non-sighted users. <br/>The alt text can remain empty only if the image conveys no meaning (is decorative only).",
        "#type": "textfield",
        "#default_value": "test image",
        "#maxlength": 2048
      },
      "data-conversion": {
        "#title": "Image size",
        "#description": "Choose the image size conversion to display.",
        "#type": "select",
        "#options": {
          "thumb": "thumb",
          "medium": "medium",
          "large": "large"
        },
        "#default_value": "center"
      },
      "data-align": {
        "#title": "Align",
        "#description": "How the image will align within the content.",
        "#type": "select",
        "#options": {
          "none": "None",
          "left": "Left",
          "center": "Center",
          "right": "Right"
        },
        "#default_value": "right"
      }
    },
    "actions": {
      "#type": "actions",
      "save_modal": {
        "#type": "submit",
        "#value": "Save",
        "#attributes": {
          "class": ["button--primary"]
        },
        "#submit": [],
        "#ajax": {
          "callback": ["Drupal\\embridge_ckeditor\\Form\\EmbridgeCkeditorImageDialog", "ajaxSave"],
          "event": "click"
        }
      }
    }
  }
}